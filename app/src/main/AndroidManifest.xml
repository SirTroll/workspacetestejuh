<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.led_on_off.led">

    <uses-permission android:name="android.permission.BLUETOOTH_ADMIN" />
    <uses-permission android:name="android.permission.BLUETOOTH" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.sirtprojects.testejuh.SplashScreen"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.sirtprojects.testejuh.DeviceList"
            android:label="@string/app_name" />
        <activity
            android:name="com.sirtprojects.testejuh.ledControl"
            android:label="LED Control" />
        <activity android:name="com.sirtprojects.testejuh.TestsActivity" />
        <activity android:name="com.sirtprojects.testejuh.LayoutActivity"></activity>
    </application>

</manifest>